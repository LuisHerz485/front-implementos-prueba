{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
    ],
    "plugins": ["@typescript-eslint", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "@typescript-eslint/no-explicit-any": "off",
        "prettier/prettier": "error"
      }
}
